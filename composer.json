{
  "name": "the-shop/fortytwo-sendgrid",
  "description": "SendGrid library for the-shop/fortytwo-* packages",
  "license": "MIT",
  "type": "library",
  "homepage": "https://github.com/the-shop/fortytwo-sendgrid",
  "authors": [
    {
      "name": "The Shop d.o.o.",
      "homepage": "http://the-shop.io/"
    }
  ],
  "require": {
    "the-shop/fortytwo-base": "^0.1.3",
    "sendgrid/sendgrid": "~6.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.3",
    "squizlabs/php_codesniffer": "^3.0"
  },
  "autoload": {
    "psr-4": {
      "Framework\\SendGrid\\": "src/",
      "Framework\\SendGrid\\Configuration\\": "config/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Framework\\SendGrid\\Test\\": "test/",
      "Framework\\Base\\Test\\": "vendor/the-shop/fortytwo-base/test/"
    }
  },
  "scripts": {
    "test": "vendor/bin/phpunit"
  }
}
